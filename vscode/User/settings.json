{
  "editor.renderWhitespace": "all",
    "editor.rulers": [80],
    "files.trimTrailingWhitespace": true,
    "editor.fontSize": 16,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "editor.tabSize": 2,
      "files.autoSave": "onFocusChange",
      "window.zoomLevel": 0,
      "html.autoClosingTags": false,
      "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "go.buildOnSave": "off",
      "go.formatTool": "gofmt",
      "go.useLanguageServer": true,
      "debug.console.fontFamily": "Source Code Pro",
      "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "workbench.tree.indent": 16,
      "ruby.format": "rubocop",
      "ruby.codeCompletion": "rcodetools",
      "ruby.intellisense": "rubyLocate",
      "ruby.useLanguageServer": true,
      "editor.selectionClipboard": false,
      "editor.dragAndDrop": false,
      "workbench.iconTheme": "easy-icons",
      "workbench.colorTheme": "Little Yeti",
      "calva.fmt.formatAsYouType": false,
      "calva.fmt.newIndentEngine": true,
      "calva.fmt.removeConsecutiveBlankLines": true,
      "telemetry.enableTelemetry": false,
      "telemetry.enableCrashReporter": false,
      "window.restoreWindows": "none",
      "files.hotExit": "off",
      "workbench.startupEditor": "newUntitledFile",
      "editor.fontLigatures": false,
      "editor.fontFamily": "\"Sauce Code Pro Nerd Font Complete\", monospace, 'Droid Sans Fallback'"
}
